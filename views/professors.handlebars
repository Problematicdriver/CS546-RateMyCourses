<div class="container-fluid pt-5">
    <div class="container">
        <div class="text-center mx-auto mb-5" style="max-width: 500px;">
            <h5 class="d-inline-block text-primary text-uppercase border-bottom border-5">Find A Professor</h5>
            <h1 class="display-4 mb-4">Find your interested professors</h1>
        </div>
        <div class="mx-auto" style="width: 100%; max-width: 600px;">
            <div class="input-group">
                <select class="form-select border-primary w-25" style="height: 60px;">
                    <option selected="">Department</option>
                    {{#each dpts}}
                    <option value={{this}}>{{this}}</option>
                    {{/each}}
                </select>
                <input type="text" class="form-control border-primary w-50" placeholder="Keyword">
                <button class="btn btn-dark border-0 w-25">Search</button>
            </div>
        </div>
    </div>
</div>


<div class="container-fluid py-5">
    <div class="container">
        <div class="row g-5">
            
            {{#each allProfessors}}
            <div class="col-lg-6 team-item">
                <div class="row g-0 bg-light rounded overflow-hidden">
                    <div class="col-12 col-sm-5 h-100">
                        <img class="img-fluid h-100" src={{this.picture}} style="object-fit: cover;">
                    </div>
                    <div class="col-12 col-sm-7 h-100 d-flex flex-column">
                        <div class="mt-auto p-4">
                            <h3>{{this.professorName}}</h3>
                            <h6 class="fw-normal fst-italic text-primary mb-4">{{this.department}}</h6>
                            <p class="m-0" name="intro">{{this.introduction}}</p>
                        </div>
                        <div class="col-12 text-center">
                            <a href={{concat '/professors/' this._id}} class="btn btn-primary rounded-pill py-2 px-3 my-3">More Details</a>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}

        </div>
    </div>
</div>


